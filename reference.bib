@article{lopezfranco/tensor-products,
    title = {Tensor products of finitely cocomplete and abelian categories},
    journal = {Journal of Algebra},
    volume = {396},
    pages = {207-219},
    year = {2013},
    issn = {0021-8693},
    doi = {https://doi.org/10.1016/j.jalgebra.2013.08.015},
    url = {https://www.sciencedirect.com/science/article/pii/S0021869313004626},
    author = {Ignacio {López Franco}},
    keywords = {Deligneʼs tensor product, Abelian category, Colimit},
    abstract = {The purpose of this article is to study the existence of Deligneʼs tensor product of abelian categories by comparing it with the well-known tensor product of finitely cocomplete categories. The main result states that the former exists precisely when the latter is an abelian category, and moreover in this case both tensor products coincide. An example of two abelian categories whose Deligneʼs tensor product does not exist is given.}
}

